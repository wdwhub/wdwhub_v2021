<div class="page-header">
  <%= link_to dining_venues_path, class: 'btn btn-default' do %>
    All Dining_venues
  <% end %>
  <%= link_to edit_dining_venue_path(@dining_venue), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show dining_venue</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
