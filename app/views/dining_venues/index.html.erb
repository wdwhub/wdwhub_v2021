


<div class="row">
  <div class="col-sm-6">
    <h1>Dining_venues</h1>
  </div>

  <div class="col-sm-6 text-end">
  <%= link_to new_dining_venue_path, class: 'btn btn-primary' do %>
    Add New Dining venue
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    
        <th>Name</th>
    

            <th>Short name</th>
            <th>Permalink</th>
            
      </tr>
    </thead>

    <tbody>
      <% @dining_venues.each do |dining_venue| %>
        <%= content_tag :tr, id: dom_id(dining_venue), class: dom_class(dining_venue) do %>
          
            <td><%= link_to dining_venue.name, dining_venue %></td>
          

                      <td><%= dining_venue.short_name %></td>
                      <td><%= dining_venue.permalink %></td>
          
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
